
X= input('Enter the real part of the  complex no.= ');
Y= input('Enter the imaginary part of the complex no.= ');
P= input('Enter the real part of the  complex no.= ');
Z=complex(X,Y);
disp(Z);
plot(X,Y,'*b','MarkerSize',40);
text(X+.06,Y,'Complex Number');
xlabel('Real-axis on x axis :'); 
ylabel('Imaginary-axis on y axis :');
